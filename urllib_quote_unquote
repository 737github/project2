# -*- coding: UTF-8 -*-
from urllib.parse import quote, unquote

#中文编码
keyword = '反恐精英全球开挂'
url = 'http://www.baidu.com=' + quote(keyword)
print("quote:", url)

#中文解码

url = 'http://www.baidu.com=%E5%8F%8D%E6%81%90%E7%B2%BE%E8%8B%B1%E5%85%A8%E7' \
      '%90%83%E5%BC%80%E6%8C%82'
print("unquote:", unquote(url))

